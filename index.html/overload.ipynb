    "## implementing simple method overloading\n",
    "\n",
    "class Arithmetic:\n",
    "    def add(self, a, b, c=0, d=0): \n",
    "        return a + b + c + d\n",
    "\n",
    "ari = Arithmetic()\n",
    "print(ari.add(10, 5))        \n",
    "print(ari.add(10, 5, 3))     \n",
    "print(ari.add(10, 5, 3, 2))  \n"
